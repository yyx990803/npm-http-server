{
  "name": "npm-http-server",
  "version": "2.15.0",
  "description": "Serve files from npm packages over HTTP",
  "repository": "mjackson/npm-http-server",
  "author": "Michael Jackson",
  "license": "MIT",
  "main": "lib",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint modules",
    "build": "node ./scripts/build.js",
    "build-lib": "rimraf lib && babel ./modules -d lib --copy-files",
    "release": "node ./scripts/release.js",
    "prepublish": "node ./scripts/build.js"
  },
  "dependencies": {
    "archiver": "^1.0.0",
    "byte-size": "^2.0.0",
    "csso": "^2.0.0",
    "gunzip-maybe": "^1.2.1",
    "http-client": "^4.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.0.1",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "os-tmpdir": "^1.0.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "semver": "^5.0.3",
    "tar-fs": "^1.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^3.1.1",
    "eslint-plugin-import": "^1.7.0",
    "eslint-plugin-react": "^5.1.1",
    "in-publish": "^2.0.0",
    "readline-sync": "^1.4.1",
    "rimraf": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015-loose",
      "stage-1",
      "react"
    ]
  }
}
